{
  "name": "log-system-serverless",
  "version": "1.0.0",
  "description": "This project generated using serverless node generator",
  "author": "Naeem",
  "main": "src/index.ts",
  "private": false,
  "license": "MIT",
  "engines": {
    "node": ">=10",
    "yarn": "*"
  },
  "scripts": {
    "dev": "ts-node-dev --no-notify node_modules/.bin/sls offline start --noTimeout",
    "tsc": "tsc",
    "deploy": "node_modules/.bin/sls deploy",
    "lint": "tslint '*.ts'",
    "lint:fix": "yarn lint -- --fix",
    "watch": "tsc -w"
  },
  "keywords": [
    "serverless",
    "node",
    "typescript",
    "es6",
    "log-system",
    "eslint",
    "REST",
    "API",
    "starter project",
    "aws",
    "lambda",
    "serverless-framework"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/naeemark/log-system-serverless.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "_moduleAliases": {
    "@config": "src/config",
    "@middlewares": "src/middlewares",
    "@services": "src/services",
    "@utils": "src/utils"
  },
  "dependencies": {
    "@hapi/joi": "^15.0.3",
    "cross-env": "^5.2.0",
    "es-class": "^2.1.1",
    "http-status": "^1.3.2",
    "lambda-log": "^2.3.0",
    "lodash": "^4.17.19",
    "middy": "^0.27.0",
    "module-alias": "^2.0.6"
  },
  "devDependencies": {
    "@types/node": "^14.0.13",
    "aws-sdk": "^2.700.0",
    "nodemon": "^2.0.4",
    "serverless": "^1.70.0",
    "serverless-offline": "^6.1.5",
    "serverless-plugin-typescript": "^1.1.9",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.49",
    "tslint": "^6.1.2",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.9.5"
  }
}
